<Window x:Class="BIMoQ.WallTypeSelectionWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Select Wall Type" Height="200" Width="400">
    <Grid>
        <StackPanel Margin="10">
            <TextBlock Text="Select Wall Type:" Margin="0,0,0,10" />
            <ComboBox x:Name="WallTypeComboBox" Height="30" />
            <Button Content="OK" Width="100" Height="30" Margin="0,10,0,0" HorizontalAlignment="Center" Click="OKButton_Click"/>
        </StackPanel>
    </Grid>
</Window>
